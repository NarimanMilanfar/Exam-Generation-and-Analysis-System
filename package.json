{
  "name": "team-8-capstone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:coverage": "node scripts/open-coverage.js",
    "test:coverage:watch": "cross-env COVERAGE=true jest --coverage --watch",
    "test:coverage:open": "node scripts/open-coverage.js",
    "test:coverage:ci": "cross-env COVERAGE=true jest --coverage --watchAll=false --passWithNoTests",
    "db:studio": "prisma studio --schema=database/prisma/schema.prisma",
    "db:check": "node scripts/check-db.js",
    "db:generate": "prisma generate --schema=./database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./database/prisma/schema.prisma",
    "test:watch": "jest --watch",
    "db:seed": "tsx database/prisma/seed.ts",
    "db:migrate": "prisma migrate dev --schema=database/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=database/prisma/schema.prisma",
    "dev:setup": "docker-compose up -d db && sleep 3 && npx prisma generate --schema=database/prisma/schema.prisma && npx prisma migrate deploy --schema=database/prisma/schema.prisma && npm run db:seed",
    "dev:full": "npm run dev:setup && npm run dev"
  },
  "prisma": {
    "seed": "tsx database/prisma/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
