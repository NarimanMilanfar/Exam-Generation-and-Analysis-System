{
  "extends": "./tsconfig.project.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "app/types/*.d.ts",
    "test/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}